<FrameLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:maskimage="http://schemas.android.com/apk/res/com.mpqi.myjiujiujintu"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical" >

        <!--
    <com.mpqi.myjiujiujintu.widget.CustomViewPager
        android:id="@+id/mainviewPager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1" /> -->

        <FrameLayout
            android:id="@+id/main_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1" >
        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingBottom="5dip"
            android:paddingTop="5dip" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <com.mpqi.myjiujiujintu.widget.MaskImage
                    android:id="@+id/mainbar1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onclcikMainrBar"
                    maskimage:image="@drawable/mainbarbackground_green"
                    maskimage:mask="@drawable/mainbar1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <com.mpqi.myjiujiujintu.widget.MaskImage
                    android:id="@+id/mainbar2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onclcikMainrBar"
                    maskimage:image="@drawable/mainbarbackground_norlmal"
                    maskimage:mask="@drawable/mainbar2" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <com.mpqi.myjiujiujintu.widget.MaskImage
                    android:id="@+id/mainbar3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onclcikMainrBar"
                    maskimage:image="@drawable/mainbarbackground_norlmal"
                    maskimage:mask="@drawable/mainbar3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <com.mpqi.myjiujiujintu.widget.MaskImage
                    android:id="@+id/mainbar4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onclcikMainrBar"
                    maskimage:image="@drawable/mainbarbackground_norlmal"
                    maskimage:mask="@drawable/mainbar4" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center" >

                <com.mpqi.myjiujiujintu.widget.MaskImage
                    android:id="@+id/mainbar5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onclcikMainrBar"
                    maskimage:image="@drawable/mainbarbackground_norlmal"
                    maskimage:mask="@drawable/mainbar5" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/main_search_city_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </FrameLayout>

</FrameLayout>